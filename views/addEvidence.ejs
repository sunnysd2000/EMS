<%- include("partials/header") -%>


<h1>Evidence Details</h1>
<form action="/addEvid" method="post">

  <div class="form-group">
    <label >Case No.:</label>
    <input type="text" class="form-control" name="caseno" placeholder="Case No"required>
  </div>
  <div class="form-group">
    <label >Evidence ID:</label>
    <input type="text" class="form-control" name="evidno" placeholder="Evidence Id" required>
  </div>
  <div class="form-group">
    <label >Data:</label>
    <input type="text" class="form-control" name="data" placeholder="Data" required>
  </div>

  <div class="form-group">
    <label >Owner Name:</label>
    <input type="text" class="form-control" name="owner" placeholder="Owner Name" required>
  </div>
  

  <button type="submit" class="btn btn-primary">Submit</button>
</form>


